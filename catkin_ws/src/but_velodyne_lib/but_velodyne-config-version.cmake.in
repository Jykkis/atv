set( PACKAGE_VERSION "@BUT_VELODYNE_VERSION@" )
if( "${PACKAGE_FIND_VERSION}" VERSION_EQUAL "@BUT_VELODYNE_VERSION@")
  set(PACKAGE_VERSION_EXACT 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}.${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@BUT_VELODYNE_SOVERSION@" )
  set(PACKAGE_VERSION_COMPATIBLE 1)
endif()
if( "${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@BUT_VELODYNE_VERSION_MAJOR@" )
  if( "${PACKAGE_FIND_VERSION_MINOR}" EQUAL 0 )
    # user has not specified a minor version, so return compatible
    set(PACKAGE_VERSION_COMPATIBLE 1)
  endif()
endif()
